{
	"name": "csatf/php-whitelist-check",
	"type": "library",
	"description": "Provides a simple way to check whether an address or domain is on a whitelist",
	"keywords": ["whitelist", "ipv4", "cidr", "ipv6"],
	"homepage": "https://github.com/Jalle19/php-whitelist-check",
	"license": "BSD-2-Clause",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/csatf/ip-utils"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"csatf/ip-utils": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8"
	},
	"autoload": {
		"psr-0": {
			"Whitelist\\": "src/"
		}
	}
}
